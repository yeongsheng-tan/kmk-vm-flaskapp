# Simple default host definition for Nginx

server {

    # Standard HTTP, IPv4
    listen 80;
    server_name localhost;

    # Documents
    root /var/www/default;
    index index.html;
    error_page 404 /404.html;

    # Logging
    access_log /var/log/nginx/default-access.log;
    error_log /var/log/nginx/default-error.log;

}